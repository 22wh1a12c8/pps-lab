#include<stdio.h>
int main()
{
int n,i,*ptr, a[10],elements;
printf("enter the number of elements of array");
scanf("%d",&n);
printf("enter the elements of array");
for(i=0;i<n;i++)
{
scanf("%d",&a[i]);
}
ptr=&a[0];
for(i=0;i<n;i++)
{
printf("%d\n",*(ptr+i));
}
return 0;
}
output:
enter the number of elements of array3
enter the elements of array12 23 34
12
23
34
