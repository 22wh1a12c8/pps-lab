#include <stdio.h>
void main()
{
int number;
printf("enter number");
scanf("%d",&number);
if(number>=0)
{
printf("number is positive");
printf("%d",number);
}
else
{
printf("number is negative");
printf("%d",number);
}
{
printf("\nend of if else"); 
}
return;
}
output:
enter number4
number is positive4
end of if else
